<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aviro.demoblockchain">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Registor" />
        <activity android:name=".dashBoard" />
        <activity android:name=".chainActivity" />
        <activity android:name=".addBlockActivity" />
        <activity android:name=".permitActivity" />
        <activity android:name=".userActivity" />
        <activity android:name=".counter" />
        <activity android:name=".Blocks"></activity>
    </application>

</manifest>